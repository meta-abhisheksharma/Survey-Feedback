<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

 <hibernate-mapping>
  

  <class name="com.survey.model.Users" table="users">
      <meta attribute="class-description">       This class contains the user detail.   </meta>
    	<id name="id" type="long" column="id">   <generator class="increment"/>    </id>

    <property name="userId" column="userId" type="string" ></property>
    <property name="name" column="name" type="string" ></property>
    <property name="email" column="email" type="string"></property>
    <property name="password" column="password" type="string"></property>
    <property name="createdTime" column="createdTime" type="date"></property>
    <property name="updatedTime" column="updatedTime" type="date"></property>
    <property name="userRole" column="userRole" type="string"></property>
  	<property name="blockUser" column="blockUser" type="string"></property>
    

  </class>
  

 </hibernate-mapping>